  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "npm run prisma:generate && tsc",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate deploy",
    "prisma:rollback": "npx prisma migrate resolve --rolled-back 20250411054438_add_category_model",
    "prisma:baseline1": "npx prisma migrate resolve --applied 20250411054438_add_category_model",
    "prisma:baseline2": "npx prisma migrate resolve --applied 20250411104335_remove_campaign_model",
    "prisma:baseline3": "npx prisma migrate resolve --applied 20250411113046_add_promotion_model",
    "prisma:baseline4": "npx prisma migrate resolve --applied 20250411144203_add_campaign",
    "prisma:baseline5": "npx prisma migrate resolve --applied 20250411201024_add_asin_to_product",
    "prisma:baseline6": "npx prisma migrate resolve --applied 20250412164533_make_company_name_nullable",
    "prisma:baseline7": "npx prisma migrate resolve --applied 20250413132849_add_review_promotion_fields",
    "postinstall": "npm install prisma --save-dev && npm run prisma:generate && (npm run prisma:migrate || (npm run prisma:rollback && npm run prisma:baseline1 && npm run prisma:baseline2 && npm run prisma:baseline3 && npm run prisma:baseline4 && npm run prisma:baseline5 && npm run prisma:baseline6 && npm run prisma:baseline7))"
  },
